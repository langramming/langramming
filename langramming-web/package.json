{
  "name": "langramming-web",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.html",
  "engines": {
    "node": "^16.13.2",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "build": "rm -rf build/ dist/ && parcel build public/index.html",
    "start": "parcel serve --public-url '/langramming/assets' public/index.html"
  },
  "dependencies": {
    "evergreen-ui": "^6.7.1",
    "query-string": "^7.1.0",
    "react": "^17.0.2",
    "react-audio-player": "^0.17.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.0",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-select": "^5.2.2",
    "redux": "^4.1.2",
    "styled-components": "5.3.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-select": "^5.0.1",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "lint-staged": "^12.2.2",
    "parcel": "^2.2.1",
    "postcss": "^8.4.5",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "targets": {
    "maven": {
      "distDir": "target/parcel-build",
      "engines": {
        "browsers": "last 5 Chrome versions, last 3 Firefox versions, last 3 Safari versions AND >1%"
      }
    }
  }
}
